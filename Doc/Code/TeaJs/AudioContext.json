{
    "name": "AudioContext",
    "namespace": "TeaJs",
    "extends": null,
    "description": "音频解析类",
    "funs": [
        {
            "name": "play",
            "description": "播放音频，若播放过音频则从记录点继续播放",
            "constructor": false,
            "params": [],
            "returns": null
        },
        {
            "name": "pause",
            "description": "暂停播放，保存当前播放到的位置",
            "constructor": false,
            "params": [],
            "returns": null
        },
        {
            "name": "stop",
            "description": "停止播放，清空记录点",
            "constructor": false,
            "params": [],
            "returns": null
        },
        {
            "name": "getFrequencyValue",
            "description": "获取歌曲频率",
			"constructor": false,
            "params": [],
            "returns": {
			    "type": "Number",
				"description": "从歌曲解析出来的歌曲频率值"
			}
        }
    ],
    "attributes": [
        {
            "name": "isLoop",
            "type": "Boolean",
            "description": "是否循环播放",
            "canGet": true,
            "canSet": true,
            "default": "false"
        },
        {
            "name": "isPlaying",
            "type": "Boolean",
            "description": "是否正在播放",
            "canGet": true,
            "canSet": true,
            "default": "false"
        },
        {
            "name": "context",
            "type": "window.AudioContext",
            "description": "音频上下文对象",
            "canGet": true,
            "canSet": true,
            "default": "Object"
        },
        {
            "name": "useAnalyser",
            "type": "Boolean",
            "description": "是否使用音频分析器",
            "canGet": true,
            "canSet": true,
            "default": "false"
        }
    ]
}