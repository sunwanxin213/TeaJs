{
    "name": "Loader",
    "namespace": "TeaJs",
    "extends": null,
	"constructor": true,
    "description": "加载器类",
    "funs": [
        {
            "name": "load",
            "description": "加载资源文件",
            "params": [
			    {
				   "name": "name",
				   "type": "String",
				   "description": "标记名称",
				   "optional": false
				},
				{
				    "name": "fileName",
					"type": "String",
					"description": "要加载的文件地址",
					"optional": false
				},
				{
				    "name": "callback",
					"type": "Function",
					"description": "加载完成后的回调函数",
					"optional": false
				}
			],
            "returns": null
        },
        {
            "name": "get",
            "description": "获取已加载的资源",
            "params": [
                {
                    "name": "name",
                    "type": "String",
                    "description": "标记名称",
                    "optional": false
                }
            ],
            "returns": {
			    "type": "Object",
				"description": "已加载的资源，类型根据加载器需要返回"
			}
        },
        {
            "name": "remove",
            "description": "移除已加载的资源",
            "params": [
                {
                    "name": "name",
                    "type": "String",
                    "description": "标记名称",
                    "optional": false
                }
            ],
            "returns": null
        },
        {
            "name": "checkFormat",
            "description": "检查支持的格式",
            "params": [
                {
                    "name": "format",
                    "type": "String",
                    "description": "格式后缀名",
                    "optional": false
                }
            ],
            "returns": {
			    "type": "Boolean",
				"description": "是否支持此格式"
			}
        }
    ],
    "attributes": [
        {
            "name": "loaderName",
            "type": "String",
            "description": "加载器名称",
            "canGet": true,
            "canSet": true,
            "default": "Object"
        },
        {
            "name": "formatList",
            "type": "Array",
            "description": "加载器支持文件后缀列表",
            "canGet": true,
            "canSet": true,
            "default": "Array"
        },
        {
            "name": "itemList",
            "type": "Array",
            "description": "加载完成项列表",
            "canGet": true,
            "canSet": true,
            "default": "Array"
        },
        {
            "name": "loadNum",
            "type": "Number",
            "description": "要加载的项数量",
            "canGet": true,
            "canSet": true,
            "default": 0
        },
        {
            "name": "loadDoneNum",
            "type": "Number",
            "description": "加载完成的项数量",
            "canGet": true,
            "canSet": true,
            "default": 0
        }
	]
}